{
	"manifest_version": 3,
	"name": "Pokeclicker Menu",
	"version": "1.0.0",
	"description": "https://github.com/lumenthi/pokeclicker-menu",
	"icons": {
		"16":"icon.png",
		"48":"icon.png",
		"128":"icon.png"
	},
	"action": {
    		"default_icon": {
        		"19": "icon.png",
        		"38": "icon.png"
    		}
	},
	"content_scripts": [
 		{
 			"matches": ["https://www.pokeclicker.com/"],
			"js": ["content-script.js", "drag.js", "menu.js"],
			"css": ["menu.css"]
 		}
 	]
}
